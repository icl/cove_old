<div class="new_interval container_16">

<h1 class="grid_16">New Interval<br><br></h1>

<%= simple_form_for(@interval) do |f| %>
  <%= f.error_notification %>

  <div class=" grid_16 inputs">
    <%= f.input :filename %>
    <%= f.input :camera_angle %>
    <%= f.input :session_number %>
    <%= f.input :start_time %>
    <%= f.input :duration %>
    <%= f.input :session_type %>
    <%= f.input :phrase_name %>
    <%= f.input :phrase_type %>
    <%= f.input :task_name %>
    <%= f.input :comments %>
  </div>

  <div class="grid _16 actions">
    <%= f.button :submit %>
  </div>

</div>
<% end %>

